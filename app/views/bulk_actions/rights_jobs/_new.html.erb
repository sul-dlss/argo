<%= form_with url: rights_job_path, class: 'new_bulk_action', data: { turbo_frame: '_top' } do |f| %>
  <span class="help-block">
    Edit rights
  </span>

  <div class='mb-3'>
    <%= f.select(:rights, Constants::REGISTRATION_RIGHTS_OPTIONS) %>
  </div>

  <%= render 'bulk_actions/druids', f: f %>
  <%= render 'bulk_actions/common_fields', f: f %>
<% end %>
