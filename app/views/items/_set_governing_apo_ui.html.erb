<%= form_tag set_governing_apo_item_path(@cocina.externalIdentifier) do %>
  <div class='mb-3'>
    <%= label_tag :new_apo_id, 'New governing APO' %>
    <%= select_tag :new_apo_id,
                   options_for_select(apo_list(current_user.groups), @cocina.administrative.hasAdminPolicy),
                   class: 'form-select' %>
  </div>
  <button class='btn btn-primary'>
    Update
  </button>
<% end %>
